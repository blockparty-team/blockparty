{
  "name": "distortion",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/distortion/src",
  "projectType": "application",
  "prefix": "app",
  "generators": {},
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/distortion",
        "index": "apps/distortion/src/index.html",
        "main": "apps/distortion/src/main.ts",
        "polyfills": "apps/distortion/src/polyfills.ts",
        "tsConfig": "apps/distortion/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/distortion/src/assets",
            "output": "assets"
          },
          "apps/distortion/src/manifest.webmanifest",
          "apps/distortion/src/OneSignalSDKWorker.js"
        ],
        "styles": [
          "apps/distortion/src/global.scss",
          "apps/distortion/src/theme/variables.scss"
        ],
        "scripts": [],
        "vendorChunk": true,
        "extractLicenses": false,
        "buildOptimizer": false,
        "sourceMap": true,
        "optimization": false,
        "namedChunks": true,
        "serviceWorker": false,
        "ngswConfigPath": "ngsw-config.json"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "libs/shared/enviroments/src/lib/environment.ts",
              "with": "libs/shared/enviroments/src/lib/distortion.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "aot": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "10mb",
              "maximumError": "15mb"
            }
          ]
        },
        "development": {
          "fileReplacements": [
            {
              "replace": "libs/shared/enviroments/src/lib/environment.ts",
              "with": "libs/shared/enviroments/src/lib/distortion.dev.ts"
            }
          ],
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "aot": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "10mb",
              "maximumError": "15mb"
            }
          ]
        },
        "ci": {
          "progress": false
        }
      }
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "buildTarget": "distortion:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "distortion:build:production"
        },
        "development": {
          "buildTarget": "distortion:build:development"
        },
        "ci": {
          "progress": false
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "distortion:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/distortion/src/test.ts",
        "polyfills": "apps/distortion/src/polyfills.ts",
        "tsConfig": "apps/distortion/tsconfig.spec.json",
        "karmaConfig": "apps/distortion/karma.conf.js",
        "styles": [],
        "scripts": [],
        "assets": [
          {
            "glob": "favicon.ico",
            "input": "apps/distortion/src/",
            "output": "/"
          },
          {
            "glob": "**/*",
            "input": "apps/distortion/src/assets",
            "output": "/assets"
          },
          "apps/distortion/src/manifest.webmanifest",
          "apps/distortion/src/OneSignalSDKWorker.js"
        ]
      },
      "configurations": {
        "ci": {
          "progress": false,
          "watch": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["{projectRoot}/**/*.ts", "{projectRoot}/**/*.html"]
      }
    },
    "build-sync-prod": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/distortion/",
        "commands": [
          "npx nx run distortion:build --configuration=production",
          "npx cap sync"
        ],
        "parallel": false
      }
    },
    "build-sync-dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/distortion/",
        "commands": ["npx nx run distortion:build", "npx cap sync"],
        "parallel": false
      }
    },
    "open-xcode": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/distortion/",
        "command": "npx cap open ios"
      }
    },
    "open-android-studio": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/distortion/",
        "command": "npx cap open android"
      }
    }
  }
}
